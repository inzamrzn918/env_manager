{
    "name": "env-manager",
    "displayName": "Environment Manager",
    "icon": "resources/icon.png",
    "description": "Manage environment variables across multiple projects and environments.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "env-manager.addProject",
                "title": "Add Project",
                "icon": "$(add)"
            },
            {
                "command": "env-manager.openDashboard",
                "title": "Open Dashboard",
                "icon": "$(dashboard)"
            },
            {
                "command": "env-manager.addEnv",
                "title": "Add Environment",
                "icon": "$(plus)"
            },
            {
                "command": "env-manager.setActive",
                "title": "Set Active",
                "icon": "$(check)"
            },
            {
                "command": "env-manager.refresh",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "env-manager.addVar",
                "title": "Add Variable",
                "icon": "$(plus)"
            },
            {
                "command": "env-manager.addWorkspaceVar",
                "title": "Add Workspace Variable",
                "icon": "$(plus)"
            },
            {
                "command": "env-manager.deleteProject",
                "title": "Delete Project",
                "icon": "$(trash)"
            },
            {
                "command": "env-manager.renameProject",
                "title": "Rename Project",
                "icon": "$(edit)"
            },
            {
                "command": "env-manager.deleteEnv",
                "title": "Delete Environment",
                "icon": "$(trash)"
            },
            {
                "command": "env-manager.renameEnv",
                "title": "Rename Environment",
                "icon": "$(edit)"
            },
            {
                "command": "env-manager.deleteVar",
                "title": "Delete Variable",
                "icon": "$(trash)"
            },
            {
                "command": "env-manager.editVar",
                "title": "Edit Variable",
                "icon": "$(edit)"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "env-manager-sidebar",
                    "title": "Env Manager",
                    "icon": "resources/icon.svg"
                }
            ]
        },
        "views": {
            "env-manager-sidebar": [
                {
                    "id": "env-manager-view",
                    "name": "Projects",
                    "icon": "resources/icon.svg"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "env-manager.addProject",
                    "group": "navigation"
                },
                {
                    "command": "env-manager.openDashboard",
                    "group": "navigation"
                },
                {
                    "command": "env-manager.refresh",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "env-manager.addEnv",
                    "when": "viewItem == project",
                    "group": "inline"
                },
                {
                    "command": "env-manager.setActive",
                    "when": "viewItem == environment",
                    "group": "inline"
                },
                {
                    "command": "env-manager.addVar",
                    "when": "viewItem == environment",
                    "group": "inline"
                },
                {
                    "command": "env-manager.addWorkspaceVar",
                    "when": "viewItem == workspace-root",
                    "group": "inline"
                },
                {
                    "command": "env-manager.renameProject",
                    "when": "viewItem == project",
                    "group": "2_modification"
                },
                {
                    "command": "env-manager.deleteProject",
                    "when": "viewItem == project",
                    "group": "2_modification"
                },
                {
                    "command": "env-manager.renameEnv",
                    "when": "viewItem == environment",
                    "group": "2_modification"
                },
                {
                    "command": "env-manager.deleteEnv",
                    "when": "viewItem == environment",
                    "group": "2_modification"
                },
                {
                    "command": "env-manager.editVar",
                    "when": "viewItem == variable",
                    "group": "inline"
                },
                {
                    "command": "env-manager.deleteVar",
                    "when": "viewItem == variable",
                    "group": "inline"
                },
                {
                    "command": "env-manager.editVar",
                    "when": "viewItem == variable",
                    "group": "1_modification"
                },
                {
                    "command": "env-manager.deleteVar",
                    "when": "viewItem == variable",
                    "group": "1_modification"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3",
        "@vscode/test-electron": "^2.3.2"
    },
    "dependencies": {
        "dotenv": "^16.3.1"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs"
}